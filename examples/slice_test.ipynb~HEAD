   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class cl():\n",
    "    def __init__(self, s):\n",
    "        self.s = s\n",
    "        self.shape = len(s)\n",
    "        \n",
    "    def __mul__(self, other):\n",
    "        \n",
    "        if isinstance(other, cl):\n",
    "            return cl(self.s * other.s)\n",
    "        else:\n",
    "            return cl(self.s * other)\n",
    "        \n",
    "    def __rmul__(self, other):\n",
    "        if isinstance(other, cl):\n",
    "            return cl(self.s * other.s)\n",
    "        else:\n",
    "            return cl(self.s * other)"
   ]
  },
