    "@timeit\n",
    "def add_ch(sig, ch = 3):\n",
    "    new_sig = np.concatenate((sig, np.zeros((sig.shape[0], ch - sig.shape[1]))), 1)\n",
    "    return new_sig\n",
    "@timeit\n",
    "def add_ch2(sig, ch = 3):\n",
    "    new_sig = np.zeros((sig.shape[0], ch))\n",
    "    new_sig[:, :sig.shape[1]] = sig\n",
    "    return new_sig"
   ]
  },
